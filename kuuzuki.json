{
  "$schema": "https://kuuzuki.ai/config.json",
  "mcp": {
    "moidvk": {
      "type": "local",

      "command": ["moidvk", "serve"]
    },
    "weather": {
      "type": "local",
      "command": ["kuuzuki", "x", "@h1deya/mcp-server-weather"]
    },
    "sequential-thinking": {
      "type": "local",
      "command": ["npx", "-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "kb-mcp": {
      "type": "local",
      "command": ["kb", "serve", "--local"]
    },
    "fork-parity": {
      "type": "local",
      "command": ["fork-parity-mcp"],
      "environment": {
        "UPSTREAM_REMOTE_NAME": "upstream",
        "UPSTREAM_BRANCH": "dev",
        "LOCAL_BRANCH": "master"
      }
    },
    "image-analysis": {
      "type": "local",
      "command": ["mcp-image-server", "serve"],
      "environment": {
        "NODE_ENV": "production"
      }
    },
    "svg-generator": {
      "type": "local",
      "command": ["mcp-svg-server", "serve"]
    }
  }
}
