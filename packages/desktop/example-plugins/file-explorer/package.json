{
  "id": "file-explorer",
  "name": "File Explorer",
  "version": "1.0.0",
  "description": "A file explorer plugin for Kuuzuki Terminal",
  "author": "Kuuzuki Team",
  "main": "index.js",
  "permissions": [
    "filesystem.read",
    "filesystem.write",
    "ui.sidebar",
    "terminal.write"
  ],
  "activationEvents": [
    "onStartup"
  ],
  "contributes": {
    "commands": [
      {
        "command": "fileExplorer.openFile",
        "title": "Open File",
        "category": "File Explorer"
      },
      {
        "command": "fileExplorer.refresh",
        "title": "Refresh",
        "category": "File Explorer"
      }
    ],
    "panels": [
      {
        "id": "fileExplorer",
        "title": "Files",
        "icon": "folder",
        "position": "left",
        "priority": 100
      }
    ],
    "keybindings": [
      {
        "command": "fileExplorer.openFile",
        "key": "ctrl+o"
      }
    ]
  },
  "engines": {
    "kuuzuki": "^1.0.0"
  }
}